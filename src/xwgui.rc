                                                ID_DEBUG_TRACENATIVEFUNCTIONS
        , GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Step In\tF11",                ID_DEBUG_STEPIN, GRAYED
        MENUITEM "Step Over\tF10",              ID_DEBUG_STEPOUT, GRAYED
        MENUITEM "Step Out\tShift+F11",         ID_DEBUG_STEPOUTCF, GRAYED
        MENUITEM "Go!\tF5",                     ID_DEBUG_GO, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Show Watches\tF7",            ID_DEBUG_SHOWWATCHES, GRAYED
        MENUITEM "Show Variables\tShift+F7",    ID_DEBUG_SHOWVARIABLES
        , GRAYED
        MENUITEM "Show Call Stack\tF8",         ID_DEBUG_SHOWCALLSTACK
        , GRAYED
        MENUITEM "Show Functions\tShift+F8",    ID_DEBUG_LISTFUNCTIONS
        , GRAYED
        MENUITEM "Show Breakpoints\tF9",        ID_DEBUG_LISTBREAKPOINTS
        , GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Remove All Breakpoints\tCtrl+F9", 
                                                ID_DEBUG_REMOVEALLBREAKPOINTS
        , GRAYED
        MENUITEM "Remove All Watches\tCtrl+F7", ID_DEBUG_REMOVEALLWATCHES
        , GRAYED
    END
