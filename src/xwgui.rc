                                                ID_TOOLS_DISCONNECTACCOUNT0
        MENUITEM SEPARATOR
        MENUITEM "Toggle &Layer Display\tCtrl+L", 
                                                ID_TOOLS_TOGGLELAYERDISPLAY
    END
    POPUP "&Debug", GRAYED
    BEGIN
        MENUITEM "&Break on next AMX call\tCtrl+B", 
                                                ID_DEBUG_BREAKONNEXTAMXCALL
        , GRAYED
        MENUITEM "Break on next Legacy Trigger\tCtrl+Shift+B", 
                                                ID_DEBUG_BREAKONNEXTLEGACYTRIGGER
        , GRAYED
