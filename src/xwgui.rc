    BEGIN
        MENUITEM "&Who is online\tCtrl+W",      ID_ADMIN_WHOISONLINE
        MENUITEM SEPARATOR
        MENUITEM "&Performance Dump\tCtrl+P",   ID_ADMIN_PERFORMANCEDUMP
    END
